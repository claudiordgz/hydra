</head>
<body class="{{body_class}} {{block "customBodyClass"}}"
    data-site-layout="wide" {{!Site Layout Options: change it to 'boxed' or 'wide'  }}
    data-post-mode="multimedia" {{!Post Mode Options: change it to 'multimedia' or 'plain'  }}    
    data-post-align="left" {{!Post Align Options: change it to 'left' or 'center'  }}
	data-uppercase="true" {{!Uppercase style: change it to 'true' or 'false'  }}
    >    
    <div id="fb-root"></div>
    <!--[if lt IE 9]>
    <div class="outdated-frame">
        You are using an <strong>outdated</strong> browser. Please <a href="http://www.google.com/chrome/intl/en/landing_chrome.html">upgrade your browser</a>.
    </div>
    <![endif]-->    
    <div class="body-wrap">
        {{! Change your header type here, include:
            + header/standard
            + header/full-screen
            + header/full-screen-center-logo            
            + header/mini
        }}
        {{>header/standard}}    
        <div class="main-wrap">
            {{{body}}}
        </div>
        {{! Change footer style here, include:        
            + footer/one-column
            + footer/two-columns
            + footer/three-columns
        }}
        {{>footer/one-column}}
    </div>
    {{ghost_foot}}
    {{! The main JavaScript file for Globin }}
    <script>
        var cb = function() {
            var links = ["{{asset "css/font-awesome.min.css"}}",
                "http://fonts.googleapis.com/css?family=Droid+Sans:400,700",
                "{{asset "css/magnific-popup.min.css"}}",
                "{{asset "css/YTPlayer.css"}}",
                "{{asset "css/style.min.css"}}"];
            for(var i=0; i != links.length; ++i) {
                var l = document.createElement('link');
                l.rel = 'stylesheet';
                l.href = links[i];
                var h = document.getElementsByTagName('head')[0];
                h.parentNode.insertBefore(l, h);
            }
        };
        var raf = requestAnimationFrame || mozRequestAnimationFrame ||
                webkitRequestAnimationFrame || msRequestAnimationFrame;
        if (raf) raf(cb);
        else window.addEventListener('load', cb);
    </script>
    <script async type="text/javascript" src="{{asset "js/bootstrap.min.js"}}"></script>
    <script async type="text/javascript" src="{{asset "js/jquery.mb.YTPlayer.js"}}"></script>
    <script async type="text/javascript" src="{{asset "js/sdk.js"}}"></script>
    <script async type="text/javascript" src="{{asset "js/waveform.js"}}"></script>
    <script async type="text/javascript" src="{{asset "js/jquery.fitvids.js"}}"></script>
    <script async type="text/javascript" src="{{asset "js/jquery.magnific-popup.min.js"}}"></script>
    <script async type="text/javascript" src="{{asset "js/jqBootstrapValidation.min.js"}}"></script>
    <script async type="text/javascript" src="{{asset "js/imagesloaded.pkgd.min.js"}}"></script>
    <script async type="text/javascript" src="{{asset "js/run_prettify.min.js"}}"></script>
    <script async type="text/javascript" src="{{asset "js/moment.min.js"}}"></script>
    <script async type="text/javascript" src="{{asset "js/pace.min.js"}}"></script>
    <script async type="text/javascript" src="{{asset "js/jquery.placeholder.min.js"}}"></script>
    <script async type="text/javascript" src="{{asset "js/sscr.js"}}"></script>
    <script async type="text/javascript" src="{{asset "js/main.min.js"}}"></script>
    {{{block "customScript"}}}
</body>
</html>